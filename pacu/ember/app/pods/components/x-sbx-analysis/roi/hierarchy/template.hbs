<p class="ui header" style={{colorStyle}}>
  ROI #{{roi.id}} ({{roi.polygon.length}})</p>
<div class="ui list">
  <div class="item">
    <i class="selected radio icon"></i>
    <div class="content">({{roi.centroid.x}}, {{roi.centroid.y}})</div>
  </div>
</div>
{{#each roi.traces as |t|}}
<span class="fluid ui basic teal button inherit-font-size"
  {{action (route-action "fetchTrace" t t.category)}}>Update {{t.category}}</span>
{{/each}}

