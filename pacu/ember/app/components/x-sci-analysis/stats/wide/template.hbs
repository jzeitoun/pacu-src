<thead class="center aligned">
  <tr>
    <th rowspan="2">Cell ID</th>
    <th rowspan="2">SF</th>
    <th rowspan="2">OSI</th>
    <th rowspan="2">CV</th>
    <th rowspan="2">DSI</th>
    <th rowspan="2">Sigma</th>
    <th rowspan="2">Tau</th>
    <th rowspan="2">OPref</th>
    <th rowspan="2">RMax</th>
    <th rowspan="2">Residual</th>
    <th rowspan="2">Blank</th>
    <th rowspan="2">FFF</th>
    <th colspan="3">Anova</th>
    <th colspan="2">Bootstrap</th>
    <th colspan="2">Pixel</th>
    <th colspan="3">SF</th>
  </tr>
  <tr>
    <th class="cell-anova">All</th>
    <th class="cell-anova">F</th>
    <th class="cell-anova">P</th>
    <th class="cell-bootstrap">MEAN</th>
    <th class="cell-bootstrap">SD</th>
    <th class="cell-pixel">Value</th>
    <th class="cell-pixel">Blank</th>
    <th class="cell-sf">Pref</th>
    <th class="cell-sf">Fit</th>
    <th class="cell-sf">Ratio</th>
  </tr>
</thead>
<tbody class="center aligned">
  {{#each rois as |roi|}}
  <tr>
    <td class="{{if roi.active "positive"}} cell-cell-id"
        rowspan="{{roi.responseCountPlusOne}}">{{roi.id}}</td>
  </tr>
    {{#each-in roi.responses as |sfreq r|}}
    <tr>
      <td class="cell-sf"            >{{sfreq}}</td>
      <td class="cell-osi"           >{{or-icon "red help" r.stats.osi}}</td>
      <td class="cell-cv"            >{{or-icon "red help" r.stats.cv}}</td>
      <td class="cell-dsi"           >{{or-icon "red help" r.stats.dsi}}</td>
      <td class="cell-sigma"         >{{or-icon "red help" r.stats.sigma}}</td>
      <td class="cell-tau"           >{{or-icon "red help" r.stats.tau}}</td>
      <td class="cell-o-pref"        >{{or-icon "red help" r.stats.o_pref}}</td>
      <td class="cell-r-max"         >{{or-icon "red help" r.stats.r_max}}</td>
      <td class="cell-residual"      >{{or-icon "red help" r.stats.residual}}</td>
      <td class="cell-blank"         >{{or-icon "red help" r.stats.blank}}</td>
      <td class="cell-fff"           >{{or-icon "red help" r.stats.fff}}</td>
      <td class="cell-anova-all"     >{{or-icon "red help" r.stats.anova_all}}</td>
      <td class="cell-anova-f"       >{{or-icon "red help" r.stats.anova_f}}</td>
      <td class="cell-anova-p"       >{{or-icon "red help" r.stats.anova_p}}</td>
      <td class="cell-bootstrap-mean">{{or-icon "red help" r.stats.bootstrap_mean}}</td>
      <td class="cell-bootstrap-sd"  >{{or-icon "red help" r.stats.bootstrap_sd}}</td>
      <td class="cell-pixel-value"   >{{or-icon "red help" r.stats.pixel_value}}</td>
      <td class="cell-pixel-blank"   >{{or-icon "red help" r.stats.pixel_blank}}</td>
      <td class="cell-sf-pref"       >{{or-icon "red help" r.stats.sf_pref}}</td>
      <td class="cell-sf-fit"        >{{or-icon "red help" r.stats.sf_fit}}</td>
      <td class="cell-sf-ratio"      >{{or-icon "red help" r.stats.sf_ratio}}</td>
    </tr>
    {{/each-in}}
  {{/each}}
</tbody>
