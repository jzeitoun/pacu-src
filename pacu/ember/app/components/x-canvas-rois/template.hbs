{{#each attrs.rois as |roi|}}
  {{svg-rect
    x1    = roi.x1
    x2    = roi.x2
    y1    = roi.y1
    y2    = roi.y2
    temp  = roi.temp
    active= roi.active
    contextMenu = (action "destroyROI" roi)
    onROIClick = (action "activateROI" roi)
    onROIMove = (action "refreshROI" roi)
  }}
{{/each}}
